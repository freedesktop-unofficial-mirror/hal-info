        <match key="info.is_recalled" bool="true">
          <merge key="info.recall.vendor" type="string">LENOVO</merge>
          <merge key="info.recall.website_url" type="string">http://www.lenovo.com/batteryprogram</merge>
        </match>

      </match>
    </match>

  </device>
</deviceinfo>
