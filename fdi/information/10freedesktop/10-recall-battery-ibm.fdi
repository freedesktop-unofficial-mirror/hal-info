        <match key="battery.model" contains="92P1073">
          <merge key="info.is_recalled" type="bool">true</merge>
        </match>
        <match key="battery.model" contains="92P1088">
          <merge key="info.is_recalled" type="bool">true</merge>
        </match>
        <match key="battery.model" contains="92P1089">
          <merge key="info.is_recalled" type="bool">true</merge>
        </match>
        <match key="battery.model" contains="92P1142">
          <merge key="info.is_recalled" type="bool">true</merge>
        </match>
        <match key="battery.model" contains="92P1141">
          <merge key="info.is_recalled" type="bool">true</merge>
        </match>
        <match key="battery.model" contains="92P1170">
          <merge key="info.is_recalled" type="bool">true</merge>
        </match>
        <match key="battery.model" contains="92P1169">
          <merge key="info.is_recalled" type="bool">true</merge>
